{
  "manifest_version": 3,
  "name": "png-info-viewer",
  "version": "2.0",
  "description": "右クリックした画像の情報を表示します。",
  "permissions": [
    "contextMenus",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
